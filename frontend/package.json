{
  "name": "react-chat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "yarn clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf build",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint './src/**/*.{ts,tsx}'",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "ts": "tsc --noEmit",
    "bundle-analyzer": "cross-env NODE_ENV=production BUNDLE_ANALYZER=localhost webpack",
    "ui": "start-storybook -p 7778",
    "ui:storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn ts"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "stylelint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "axios": "0.21.1",
    "date-fns": "2.12.0",
    "final-form": "4.19.1",
    "history": "4.10.1",
    "polished": "3.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-final-form": "6.4.0",
    "react-portal": "4.2.1",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@hot-loader/react-dom": "16.13.0",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@svgr/webpack": "5.3.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-router-dom": "5.1.4",
    "@typescript-eslint/parser": "2.29.0",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "eslint-plugin-typescript": "0.14.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.1.7",
    "mini-css-extract-plugin": "0.9.0",
    "prettier": "2.0.5",
    "react-hot-loader": "4.12.20",
    "rimraf": "3.0.2",
    "source-map-loader": "0.2.4",
    "stylelint": "13.3.3",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
