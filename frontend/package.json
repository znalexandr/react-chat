{
  "name": "react-chat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "yarn clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf build",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint './src/**/*.{ts,tsx}'",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "ts": "tsc --noEmit",
    "bundle-analyzer": "cross-env NODE_ENV=production BUNDLE_ANALYZER=localhost webpack",
    "ui": "start-storybook -p 7778",
    "ui:storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn ts"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "stylelint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "antd": "3.26.12",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "date-fns": "2.9.0",
    "history": "4.10.1",
    "polished": "3.4.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-portal": "4.2.1",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@babel/runtime": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@hot-loader/react-dom": "16.11.0",
    "@storybook/addon-actions": "5.3.13",
    "@storybook/addon-links": "5.3.13",
    "@storybook/addons": "5.3.13",
    "@storybook/react": "5.3.13",
    "@svgr/webpack": "5.2.0",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/parser": "2.20.0",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "10.0.27",
    "cross-env": "7.0.0",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.4.0",
    "eslint-plugin-typescript": "0.14.0",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "mini-css-extract-plugin": "0.9.0",
    "prettier": "1.19.1",
    "react-hot-loader": "4.12.19",
    "rimraf": "3.0.2",
    "source-map-loader": "0.2.4",
    "stylelint": "13.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "typescript": "3.8.2",
    "webpack": "4.41.6",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
